"
RTGraphBuilderExample new installTitle: 'ConnectFromAndSelectiveColor' 
		code:
		'
	| b |
	b := RTGraphBuilder new.
	b nodes color: Color blue.
	b edges
		connectFrom: [ :v | v // 6 ];
		if: [ :v | v ~~ 50 ];
		color: Color gray.
		
	b edges
		connectFrom: [ :v | v // 6 ];
		if: [ :v | v = 50 ];
		color: Color red.

	b layout circle radius: 220.
	b addAll: (1 to: 100).

	b open.
	b view canvas'
	
"
Class {
	#name : #RTGraphBuilderExample,
	#superclass : #RTAbstractExample,
	#category : #'Roassal2-ExampleBrowser'
}

{ #category : #'as yet unclassified' }
RTGraphBuilderExample >> exampleConnectFromAndSelectiveColor [

	| b |
	b := RTGraphBuilder new.
	b nodes color: Color blue.
	b edges
		connectFrom: [ :v | v // 6 ];
		if: [ :v | v ~~ 50 ];
		color: Color gray.
		
	b edges
		connectFrom: [ :v | v // 6 ];
		if: [ :v | v = 50 ];
		color: Color red.

	b layout circle radius: 220.
	b addAll: (1 to: 100).

	b open.
	b view canvas
]

